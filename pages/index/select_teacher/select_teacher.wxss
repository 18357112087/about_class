page {
  width: 100%;
  height: 100%;
  background-color: RGBA(41, 75, 83, 1);
}

.content {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.logo {
  width: 280rpx;
  height: 280rpx;
}

.icon-loading {
  font-size: 80rpx;
  color: rgba(96, 234, 243, 1);
  margin-top: 100rpx;
  -webkit-animation:gogogo 1.5s infinite ease-in-out;
  /* -webkit-animation:gogogo 1s infinite linear; */
  
}

@-webkit-keyframes gogogo {
  0% {
    -webkit-transform: rotate(0deg);
  }
  /* 25% {
    -webkit-transform: rotate(60deg);
  }
  50% {
    -webkit-transform: rotate(180deg);
  }
  75% {
    -webkit-transform: rotate(300deg);
  } */
  100% {
    -webkit-transform: rotate(360deg);
  }
}

.mintitle {
  margin-top: 28rpx;
  color: rgba(255, 255, 255, 1);
  font-size: 26rpx;
}
.whitePage{
  width: 100%;
  height: 100%;
  background-color: rgba(255, 255, 255, 1);
}
page{
  width: 100%;
  height: 100%;
  background-color: RGBA(223, 243, 250, 1);
}
.head{
  width: 100%;
  height: 400rpx;
  box-sizing: border-box;
  padding: 30rpx;
}
.list_group{
   width: 100%;
   height: 660rpx;
   box-sizing: border-box;
   padding:0rpx 30rpx 100rpx 30rpx;
   background-color: rgba(255, 255, 255, 1);
}
.list_item{
   width: 100%;
  height: 110rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.list_item .iconfont {
  font-size: 36rpx;
  color: rgba(96, 234, 243, 1);
}

.list_item_title {
  font-size: 30rpx;
  color: rgba(44, 44, 44, 1);
  margin-left: 16rpx;
}
.list_item .icon-arr-right-red {
  margin-left: auto;
  font-size: 26rpx;
}
.list_item .hart{
  color: RGBA(255, 65, 109, 1);
}

.call_group{
  display: block;
  width: 240rpx;
  height: 64rpx;
  margin: 0 auto;
  margin-top: 60rpx;
}